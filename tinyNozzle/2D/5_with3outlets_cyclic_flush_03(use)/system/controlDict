FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
application     sonicFoam;

startFrom       latestTime;

startTime       0;

stopAt          endTime;

endTime         0.0003;

deltaT 		1e-08;

writeControl 	adjustableRunTime;

writeInterval   0.000003;

cycleWrite 	0;

writeFormat     ascii;

writePrecision  3;

writeCompression off;

timeFormat      general;

timePrecision   6;

adjustTimeStep 	yes;

maxCo 		0.5;

maxDeltaT 	1;

runTimeModifiable true;

